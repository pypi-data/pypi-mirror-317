[VARIABLES]
good-names= i, j, x, y, z, n
additional-builtins=
    KEYDOWN,
    K_DOWN,
    K_ESCAPE,
    K_LEFT,
    K_RIGHT,
    K_SPACE,
    K_UP,
    MOUSEBUTTONDOWN,
    MOUSEBUTTONUP,
    MOUSEMOTION,
    QUIT,

[TYPECHECK]
ignored-modules = pygame, pygame.locals

[SIMILARITIES]
ignore-imports = yes

[MESSAGES CONTROL]
disable=
  duplicate-code,
  # Parlons français, que diable !
  non-ascii-name,
