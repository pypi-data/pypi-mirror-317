useage


ASCII Art = """
bnr1   = "@@@@@@@@  @@@@@@@@   @@@@@@@@   "
bnr2   = "@@@@@@@@  @@@@@@@@  @@@@@@@@@@  "
bnr3   = "     @@!       @@!  @@!   @@@@  "
bnr4   = "    !@!       !@!   !@!  @!@!@  "
bnr5   = "   @!!       @!!    @!@ @! !@!  "
bnr6   = "  !!!       !!!     !@!!!  !!!  "
bnr7   = " !!:       !!:      !!:!   !!!  "
bnr8   = ":!:       :!:       :!:    !:!  "
bnr9   = " ::        ::       ::::::: ::  "
bnr10  = ": :       : :        : : :  :   "
bnr11  = ""
bnr12  = "Made by Anvil <3"



def banner()
    770(bnr0)
    770(bnr1)
    770(bnr2)
    770(bnr3)
    770(bnr4)
    770(bnr5)
    770(bnr6)
    770(bnr7)
    770(bnr8)
    770(bnr9)
    770(bnr10)
    770(bnr11)

banner()
