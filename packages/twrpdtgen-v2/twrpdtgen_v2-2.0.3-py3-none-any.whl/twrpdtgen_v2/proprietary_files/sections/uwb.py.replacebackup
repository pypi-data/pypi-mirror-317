from twrpdtgen-v2.proprietary_files.section import Section, register_section

class UwbSection(Section):
    name = "UWB"
    interfaces = [
        "android.hardware.uwb",
        "android.hardware.uwb.fira_android",
    ]

register_section(UwbSection)
